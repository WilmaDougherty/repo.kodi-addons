<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.espn_3"
       name="ESPN 3"
       version="2.9.24"
       provider-name="awaters1,locomot1f,MetalChris,BlueCop,Romans I XVI">
    <requires>
        <import addon="xbmc.python" version="2.20.0"/>
        <import addon="script.module.elementtree" version="1.2.8"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
        <import addon="script.module.m3u8" version="0.2.2"/>
        <import addon="script.module.dateutil" version="2.4.2"/>
        <import addon="script.module.requests" version="2.9.1" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <language/>
        <platform>all</platform>
        <license>GNU GENERAL PUBLIC LICENSE. Version 3, 29 June 2007</license>
        <summary lang="en">ESPN 3</summary>
        <description lang="en">Unofficial addon to Watch ESPN. Watch full live streaming sporting events and replays on
ESPN 3 (and premium channels with a valid cable provider), including football, baseball, cricket, soccer,
and basketball events. NOT SUPPORTED BY ESPN OR DISNEY OR ANYONE ELSE
        </description>
        <forum>http://forum.kodi.tv/showthread.php?tid=230418</forum>
        <source>https://github.com/kodi-addons/plugin.video.espn_3</source>
        <email>bgunteriv@gmail.com</email>
    </extension>
</addon>

<addon id="repository.kodi-addons" name="Custom Kodi Addons Repo" version="1.0.0" provider-name="kodi-addons">
	<extension point="xbmc.addon.repository" name="Custom Kodi Addons Repo">
		<info compressed="false">https://raw.githubusercontent.com/kodi-addons/repo.kodi-addons/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/kodi-addons/repo.kodi-addons/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/kodi-addons/repo.kodi-addons/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Custom Kodi Addons Repo</summary>
		<description>Custom Kodi Addons Repo</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
